" filename completion
if has("wildmenu")
    set wildignore+=*.a,*.o
    set wildignore+=*.bmp,*.gif,*.ico,*.jpg,*.png
    set wildignore+=.DS_Store,.git,.hg,.svn
    set wildignore+=*~,*.swp,*.tmp
    set wildmenu
    set wildmode=longest,list:full
endif

" PYTHONPATH (for powerline)
" let $PYTHONPATH="$PYTHONPATH:/usr/lib/python3.4/site-packages"
